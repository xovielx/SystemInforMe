<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Description>A simple .exe command line application used for returning Console output regarding System Information queried from the WMI.</Description>
    <Authors>Oviel Bayola</Authors>
    <Company>N/A</Company>
    <Product>N/A</Product>
    <Copyright>2020</Copyright>
    <SignAssembly>false</SignAssembly>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\WMI.txt" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\WMI.txt" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Management" Version="5.0.0" />
  </ItemGroup>

</Project>
